{
  "name": "contacts-manager",
  "version": "1.0.0",
  "description": "Admin panel ui for Contacts Manager wordpress plugin",
  "main": "src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "node hmr.js disable && webpack --progress",
    "build": "node hmr.js disable && webpack --progress --mode production",
    "watch": "node hmr.js disable && webpack --watch --progress",
    "watch:hot": "node hmr.js enable && webpack-dev-server --hot --progress --mode development",
    "export": "webpack --progress --mode production && node export.js"
  },
